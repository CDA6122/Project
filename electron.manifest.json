{
  "executable": "Project",
  "splashscreen": {
    "imageFile": "/wwwroot/images/fau.jpg"
  },
  "singleInstance": false,
  "build": {
    "appId": "com.Project.app",
    "productName": "Project",
    "copyright": "Copyright Â© 2019 David Bruck and Freguens Mildort",
    "buildVersion": "0.0.3",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ],
    "win": {
      "icon": "../../../fau.ico"
    },
    "linux": {
      "icon": "../../../fau.png"
    },
    "mac": {
      "icon": "../../../fau.png"
    }
  }
}